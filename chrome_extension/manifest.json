{
    "manifest_version": 3,
    "name": "first_extension_woooo",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_title": "heyaa"
    }
}
